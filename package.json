{
  "name": "@saturation-api/js",
  "version": "0.0.7",
  "description": "TypeScript SDK for the Saturation API",
  "keywords": [
    "saturation",
    "api",
    "sdk",
    "typescript",
    "project-management",
    "budgeting"
  ],
  "homepage": "https://github.com/saturation/saturation-js#readme",
  "bugs": {
    "url": "https://github.com/saturation/saturation-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saturation/saturation-js.git"
  },
  "license": "MIT",
  "author": "Saturation",
  "type": "module",
  "exports": {
    ".": {
      "browser": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf dist",
    "dev": "tsc -w -p tsconfig.esm.json",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "generate": "npx @hey-api/openapi-ts",
    "generate:old": "tsx scripts/generate-from-openapi.ts",
    "lint": "eslint src --ext .ts",
    "playground": "tsx playground.ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "optionalDependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.80.8",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
